#include <stdio.h>
#include <sys/types.h>
#ifdef	OS_HPUX_IA64
#undef _INCLUDE_HPUX_SOURCE
#endif
#include <sys/ipc.h>
#include <sys/shm.h>

int	sizeok(n)
int	n;
{
	int	kk= shmget(IPC_PRIVATE, n, 0666);
	if  (kk < 0)
		return  0;
	(void) shmctl(kk, IPC_RMID, 0);
	return  1;
}

main()
{
	int	mins = 4096, maxs = 0x8000000, middle;
	FILE	*f = fopen("conftestshm", "w");

	if  (sizeok(maxs))  {
		(void) fprintf(f, "%d\n", maxs);
		exit(0);
	}
	if  (!sizeok(mins))  {
		(void) fprintf(f, "imposs\n");
		exit(1);
	}
	while  (mins < maxs)  {
		middle = (mins + maxs) / 2;
		if  (sizeok(middle))  {
			if  (mins == middle)
				break;
			mins = middle;
		}
		else  {
			if  (maxs == middle)
				break;
			maxs = middle;
		}
	}
	(void) fprintf(f, "%d\n", mins);
	exit(0);
}

