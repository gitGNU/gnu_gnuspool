#ifdef	OS_HPUX_IA64
#undef _INCLUDE_HPUX_SOURCE
#endif
#include <sys/ipc.h>
#include <sys/msg.h>

#ifndef	MSGSIZE
#define	MSGSIZE	40	/* Size is a coincidence */
#endif

struct	x	{
	long	a;
	char	stuff[MSGSIZE];
}  y;

main(argc, argv) int argc; char **argv;
{
	int	nmsgs = 40, cnt;
	int	msg_chan;
	FILE	*f = fopen("conftestmsg", "w");
	if  (argc > 1)
		nmsgs = atoi(argv[1]);
	if  ((msg_chan = msgget(0x12345678, 0666|IPC_CREAT)) < 0)
		exit(2);
	y.a = 999;
	for  (cnt = 0;  cnt < MSGSIZE;  cnt++)
		y.stuff[cnt] = 'T';
	cnt = 0;
	while  (msgsnd(msg_chan, (void *) &y, MSGSIZE, IPC_NOWAIT) >= 0)
		cnt++;
	msgctl(msg_chan, IPC_RMID, (struct msqid_ds *) 0);
	fprintf(f, "%d\n", cnt);
	fclose(f);
	return  0;
}


